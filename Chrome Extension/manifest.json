{
  "manifest_version": 2,

  "name": "Equivant 508 Compliance",
  "description": "508 Compliance plugin for testing CourtView web applications",
  "version": "1.3",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "http://localhost:8080/*",
    "http://daviesw10:8080/*"
  ],
  "content_scripts": [{
      "matches": ["http://localhost:8080/*", "http://daviesw10:8080/*"],
      "js": ["contentscript.js"]
  }],
  "web_accessible_resources": ["insertAxe.js"]
}