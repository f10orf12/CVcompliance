{
  "manifest_version": 2,

  "name": "Equivant 508 Compliance",
  "description": "508 Compliance plugin for testing web applications",
  "version": "1.0.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "content_scripts": [{
      "matches": ["http://*/*",  "https://*/*"],
      "js": ["contentscript.js"]
  }],
  "web_accessible_resources": ["insertAxe.js"]
}
